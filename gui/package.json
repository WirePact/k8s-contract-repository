    "ts-loader": "9.3.1",
    "ts-proto": "1.121.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3",
    "webpack-subresource-integrity": "5.1.0"
  }
