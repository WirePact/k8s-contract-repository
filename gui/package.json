    "long": "5.2.0",
    "nice-grpc-common": "1.1.0",
    "nice-grpc-web": "1.1.0",
    "protobufjs": "7.0.0",
    "tailwindcss": "3.1.7"
  },
  "devDependencies": {
    "autoprefixer": "10.4.8",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.0.0",
    "del-cli": "5.0.0",
    "eslint": "8.21.0",
